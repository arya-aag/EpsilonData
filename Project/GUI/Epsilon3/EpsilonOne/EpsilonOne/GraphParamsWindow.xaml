<Window x:Class="EpsilonOne.GraphParamsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpsilonOne"
        mc:Ignorable="d"
        Title="GraphParamsWindow" Height="300" Width="300">
    <Grid>
        <DatePicker HorizontalAlignment="Left" Margin="30,64,0,0" VerticalAlignment="Top" FirstDayOfWeek="Monday" DisplayDate="2010-11-05" DisplayDateStart="2009-08-21" DisplayDateEnd="2010-11-05"/>
        <DatePicker HorizontalAlignment="Left" Margin="161,64,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnOkay" Content="OK" HorizontalAlignment="Left" Margin="52,197,0,0" VerticalAlignment="Top" Width="75" Click="OkayClick"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="149,197,0,0" VerticalAlignment="Top" Width="75" Click="CancelClick"/>

    </Grid>
</Window>
